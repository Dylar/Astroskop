<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/activity_analysis_filter_hamburg_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/activity_analysis_filter_hamburg"
            style="@style/AppTheme.MediumFont"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:padding="@dimen/two_grid_unit"
            android:text="Hamburg gesamt"
            android:textColor="@color/text_gray"
            android:textSize="@dimen/text_size_small" />

        <ImageView
            android:id="@+id/activity_analysis_filter_hamburg_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:padding="@dimen/two_grid_unit"
            android:src="@drawable/ic_checkmark"
            android:visibility="gone" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_below="@+id/activity_analysis_filter_hamburg"
            android:background="@color/underline" />
    </RelativeLayout>

